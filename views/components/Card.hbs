<article class='card mb-2'>
	<div class='card-body'>
		<!--Para hacerlo de manera dinamica usamos las props del arreglo Urls-->
		<p>{{item.origin}}</p>
		<p>{{item.shortUrl}}</p>
		<a href='/eliminar/{{item._id}}' class='btn btn-danger'>Eliminar</a>
		<!--  item._id viene de mongodb     -->
		<a href='/editar/{{item._id}}' class='btn btn-warning'>Editar</a>
		<button data-short='{{item._id}}' href='#' class='btn btn-info'>Copiar</button>
	</div>
</article>